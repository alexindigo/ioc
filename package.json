{
  "version": "2.0.0",
  "name": "ioc",
  "description": "IoC library for React, with support for NextJS",
  "author": "Alex Indigo <iam@alexindigo.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/alexindigo/ioc.git"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "NODE_ENV=test jest",
